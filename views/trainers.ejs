<html>
  <head>
    <meta charset="utf-8">
    <link rel='stylesheet' href='/stylesheets/index.css' />
    <title>Trainers</title>
    <script src="https://use.fontawesome.com/44d41c7531.js"></script>

  </head>
  <body>
    <div class='top-nav'>
      <li><a href="/trainers">Trainers</a></li>
      <li><a href="/pokemon">Pokemon</a></li>
      <li><a href="/gym">Gym</a></li>
      <li></li>
      <li></li>
    </div>
    <div class="main">
      <h2>Trainers</h2>
      <img src=""></img><br>
      <div class="pokemon-list-container">
        <div class="pokemon-container">
          <% for(i= 0; i < trainerObj.length;i++ ){ %>
              <div class="pokemon">
                <div class="info"><a href="/trainers/<%=trainerObj[i].id %>"> <%=trainerObj[i].id %></a></div>
                <div class="info"><%=trainerObj[i].name %></div>
              </div>
          <%}%>
        </div>
      </div>
    </div>

  </body>
</html>
